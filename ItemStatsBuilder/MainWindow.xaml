<Window x:Class="ItemStatsBuilder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ItemStatsBuilder"
        mc:Ignorable="d"
        Title="Item Stats Viewer" Height="450" Width="800">
    <Grid>
        <Label x:Name="itemLabel" Content="Item" HorizontalAlignment="Left" Margin="45,29,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="itemCombo" HorizontalAlignment="Left" Margin="72,60,0,0" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
